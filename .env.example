# ==============================================
# Database PostgreSQL Settings
# ==============================================
POSTGRES_DB=your_database_name
POSTGRES_USER=your_postgres_username
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5433

# ==============================================
# Redis Settings
# ==============================================
REDIS_PASSWORD=your_redis_password_here
REDIS_PORT=6379

# ==============================================
# RabbitMQ Settings
# ==============================================
RABBITMQ_PORT_AMQP=5672
RABBITMQ_PORT_MANAGEMENT=15672

# ==============================================
# Application Services Ports
# ==============================================
AUTH_PORT=50051
APP_PORT=8080

# ==============================================
# File System Paths (Windows)
# ==============================================
POSTGRES_DATA=C:/path/to/your/postgres/data
REDIS_DATA=C:/path/to/your/redis/data
CONFIG_PATH_VOLUMES=C:/path/to/your/config/config.yaml
CONFIG_PATH=/config/config.yaml 
